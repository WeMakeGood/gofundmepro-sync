{
  "name": "classy-sync",
  "version": "2.0.0",
  "description": "Clean Classy (GoFundMe Pro) data synchronization system with plugin architecture for third-party integrations",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:rollback": "knex migrate:rollback",
    "db:reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
    "db:status": "knex migrate:status",
    "sync": "node src/cli.js sync",
    "org:add": "node src/cli.js org add",
    "org:list": "node src/cli.js org list",
    "org:sync": "node src/cli.js org sync",
    "mailchimp:sync": "node src/cli.js plugin mailchimp sync"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:WeMakeGood/gofundmepro-sync.git"
  },
  "keywords": [
    "gofundme",
    "classy",
    "donations",
    "sync",
    "fundraising",
    "mailchimp",
    "donor-analytics"
  ],
  "author": "We Make Good LLC",
  "license": "PROPRIETARY",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "knex": "^3.1.0",
    "mysql2": "^3.14.1",
    "sqlite3": "^5.1.7",
    "winston": "^3.11.0",
    "yargs": "^17.7.2",
    "lodash": "^4.17.21",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "faker": "^5.5.3",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
