{
  "name": "gofundmepro-sync",
  "version": "1.0.0",
  "description": "Universal database synchronization system for GoFundMe Pro (Classy) with MailChimp integration, advanced donor analytics, and automated data integrity validation",
  "main": "daemon.js",
  "scripts": {
    "start": "node daemon.js",
    "dev": "NODE_ENV=development node daemon.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:quick": "node scripts/test-db-type.js && node scripts/test-supporter-stats.js",
    "lint": "eslint src/",
    "init-db": "node scripts/init-db.js",
    "manual-sync": "node scripts/manual-sync.js",
    "initial-sync": "node scripts/initial-sync.js",
    "migrate": "node scripts/migrate.js",
    "mailchimp-sync": "node scripts/mailchimp-full-sync.js",
    "health-check": "node scripts/health-check.js",
    "db:init": "node scripts/knex-init.js init",
    "db:seed": "node scripts/knex-init.js seed", 
    "db:reset": "CONFIRM_RESET=yes node scripts/knex-init.js reset",
    "db:setup": "CONFIRM_RESET=yes node scripts/knex-init.js setup",
    "db:status": "node scripts/knex-init.js status",
    "db:validate": "node scripts/knex-init.js validate",
    "db:test": "node scripts/test-knex-flexibility.js",
    "fix:supporter-stats": "node scripts/recalculate-supporter-stats.js recalculate",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "deploy:production": "pm2 deploy ecosystem.config.js production",
    "deploy:staging": "pm2 deploy ecosystem.config.js staging"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:WeMakeGood/gofundmepro-sync.git"
  },
  "keywords": [
    "gofundme",
    "classy",
    "donations",
    "sync",
    "fundraising",
    "mailchimp",
    "donor-analytics"
  ],
  "author": "We Make Good LLC",
  "license": "PROPRIETARY",
  "dependencies": {
    "axios": "^1.6.0",
    "bullmq": "^4.15.0",
    "dotenv": "^16.3.1",
    "knex": "^3.1.0",
    "mysql2": "^3.14.1",
    "node-cron": "^3.0.3",
    "redis": "^4.6.10",
    "sqlite3": "^5.1.7",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "faker": "^5.5.3",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
